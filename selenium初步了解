#导入selenium测试框架
from selenium import webdriver
import time
#使用selenium打开火狐浏览器
driver = wbdriver.Firefox()
#使用selenium浏览页面
driver.get(url)
#通过页面内容对应的ID访问页面内容
driver.find_element_by_css_selector("#(ID)").(click（打开按钮）/send_keys(填写内容))
#通过页面内容对应的CSS访问页面内容
driver.find_element_by_class_name("class").(click(打开按钮)/send_keys(填写内容))X
#通过xpath访问页面  检查-鼠标右键-复制-xpath
driver.find_element_by_xpath('复制的xpath内容').(click(打开按钮)/send_keys(填写内容))
#显性暂停
WebDriverWait(driver, 30, 1).until(EC.presence_of_element_located((By.LINK_TEXT,"(能打开跳转页面的内容)")))
